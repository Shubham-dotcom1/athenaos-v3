# AthenaOS UI — Environment Variables
# Copy this to .env.local and fill in your real values

# Firebase (required for Auth — get from Firebase Console)
NEXT_PUBLIC_FIREBASE_API_KEY=placeholder-replace-with-real-key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=placeholder.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=placeholder-project
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=placeholder.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=000000000000
NEXT_PUBLIC_FIREBASE_APP_ID=1:000000000000:web:placeholder

# AthenaOS Backend API URL
NEXT_PUBLIC_API_URL=http://localhost:8000

# Supabase (if used)
# NEXT_PUBLIC_SUPABASE_URL=
# NEXT_PUBLIC_SUPABASE_ANON_KEY=
